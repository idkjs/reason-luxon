{
  "name": "reason-luxon",
  "namespace": "Luxon",
  "gentypeconfig": {
		"language": "typescript",
		"module": "es6",
		"importPath": "relative",
		"shims": {
			"Js": "Js",
			"React": "ReactShim",
			"ReactEvent": "ReactEvent",
			"ReasonPervasives": "ReasonPervasives",
			"ReasonReact": "ReactShim"
		},
		"debug": {
			"all": false
		},
		"exportInterfaces": true,
		"fileHeader": "effector reason"
	},
  "sources": [{ "dir": "src" }, { "dir": "test", "type": "dev" }],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "bsc-flags": [
    "-bs-g",
    "-bs-super-errors",
    "-warn-error",
    "@A",
    "-w",
    "-4-9-30-40-41-42-44-101-102"
  ],
  "warnings": {
    "error": "+101"
  },
  "refmt": 3
}
